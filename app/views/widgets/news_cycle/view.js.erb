var $widget_content = $("<%= raw escape_javascript( render :partial => "widgets/news_cycle/view" )%>");
widgetObj = widget("<%= @widget.guid %>");
widgetObj.setWidgetType("<%= @widget_slug %>")
widgetObj.setContent($widget_content);
widgetObj.setMode("normal");

$widget_content.each(function() {
  var $this = $(this);
  $this.easyAccordion({
    autoStart: true,
    slideInterval: 3000,
    slideNum: false	
  });
});

